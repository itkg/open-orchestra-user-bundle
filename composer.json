{
    "name": "itkg/open-orchestra-user-bundle",
    "description": "An override of FosUserBundle to provide authentication on OpenOrchestra",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Thal",
            "email": "nico.th4l@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/itkg/open-orchestra-model-interface"
        },
        {
            "type": "vcs",
            "url": "https://github.com/itkg/open-orchestra-display-bundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/itkg/open-orchestra-base-bundle"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "friendsofsymfony/user-bundle": "~2.0"
    },
    "require-dev": {
        "itkg/open-orchestra-base-bundle": "dev-master",
        "itkg/open-orchestra-display-bundle": "dev-master",
        "phpunit/phpunit" : "4.0.14",
        "scrutinizer/ocular": "1.1.1",
        "phake/phake": "2.0.0-beta2"
    },
    "suggest": {
        "itkg/open-orchestra-display-bundle": "In order to use all the functionality offered by the OpenOrchestra project"
    },
    "config" : {
        "bin-dir" : "bin"
    },
    "autoload": {
        "psr-0": {
            "OpenOrchestra\\UserBundle": ""
        }
    },
    "target-dir": "OpenOrchestra"
}
