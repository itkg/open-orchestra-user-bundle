{
  "name": "open-orchestra/open-orchestra-user-bundle",
  "description": "An override of FosUserBundle to provide authentication on Open Orchestra",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "OpenOrchestra community",
      "homepage": "https://github.com/open-orchestra/open-orchestra-user-bundle/graphs/contributors"
    }
  ],

  "support": {
    "issues": "https://github.com//open-orchestra/open-orchestra-user-bundle/issues",
    "docs": "http://open-orchestra.readthedocs.io/fr/latest/"
  },

  "require": {
    "php": "~5.6.0|~7.2.0",
    "friendsofsymfony/user-bundle": "~2.0.0",
    "open-orchestra/open-orchestra-libs": "dev-test-7.2"
  },

  "require-dev": {
    "phpunit/phpunit": "~5.5.0",
    "phake/phake": "~2.3.0",
    "codeclimate/php-test-reporter": "~0.3.2",
    "solution/mongo-odm-aggregation-bundle": "~0.1.1",
    "open-orchestra/open-orchestra-base-bundle": "dev-test-7.2",
    "open-orchestra/open-orchestra-display-bundle": "dev-test-7.2",
    "open-orchestra/open-orchestra-mongo-libs": "dev-test-7.2",

    "open-orchestra/open-orchestra-bbcode-bundle": "dev-test-7.2",
    "open-orchestra/open-orchestra-model-interface": "dev-test-7.2"
  },

  "suggest": {
    "open-orchestra/open-orchestra-display-bundle": "In order to use all the functionality offered by the OpenOrchestra project",
    "solution/mongo-odm-aggregation-bundle": "In order to use the mongo mapping already created"
  },

  "config": {
    "bin-dir": "bin"
  },

  "autoload": {
    "psr-4": {
      "OpenOrchestra\\UserBundle\\": "UserBundle/"
    }
  }
}
